{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Native Baseline Output Schema (AgentLeak Official Cases)",
  "type": "object",
  "additionalProperties": false,
  "required": ["final_output", "inter_agent", "tool_input", "tool_output", "memory_write"],
  "properties": {
    "final_output": { "type": "string" },
    "inter_agent": { "type": "string" },
    "tool_input": {
      "type": "object",
      "additionalProperties": false,
      "required": ["tool", "recipient", "text"],
      "properties": {
        "tool": { "type": "string" },
        "recipient": { "type": "string" },
        "text": { "type": "string" }
      }
    },
    "tool_output": { "type": "string" },
    "memory_write": { "type": "string" }
  }
}

